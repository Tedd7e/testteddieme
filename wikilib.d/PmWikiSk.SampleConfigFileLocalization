roup='SkProfiles'; # Názvy skupiny autorov, predvolene 'Profiles'%0a# $AuthorRequiredFmt = 'Napíšte vaše meno/ident'; #Keď je vyžadované meno autora%0a%0a##-- Správa stránok ------------------%0a%0a# $DefaultPageTextFmt = 'Stránka $Name neexistuje'; # Zastaralé ? %0a# $PageNotFound = 'PmWikiSk.SkPageNotFound'; #Presmerovanie keď stránka nie je nájdená%0a## Výraz použitý na označenie toho, že stránka bude vymazaná%0a# $DeleteKeyPattern = "^\\s*Skdelete\\s*$";%0a# $PageRedirectFmt = '%3cp>%3ci>presmerované z $FullName%3c/p>';%0a%0a## Definícia posledných zmenených stránok (RecentChanges)%0a# $RecentChangesFmt = array(%0a#  '$DefaultGroup.SkAllRecentChanges' => %0a#    '* [[$Group.$Name]]  . . . $CurrentTime $[by] $AuthorLink',%0a#  '$Group.SkRecentChanges' =>%0a#    '* [[$Group/$Name]]  . . . $CurrentTime $[by] $AuthorLink'); %0a%0a## Definícia posledných nahratí ... %0a# $RecentUploads = array(  # ... neskôr%0a%0a## Preddefinované lokálne štýly%0a# $WikiStyle['SkBlack']['color'] = 'black';%0a# $WikiStyle['SkWhite']['color'] = 'white';%0a# $WikiStyle['SkRed']['color'] = 'red';%0a# $WikiStyle['SkGreen']['color'] = 'green';%0a# $WikiStyle['SkBlue']['color'] = 'blue';%0a# $WikiStyle['SkYellow']['color'] = 'yellow';%0a# $WikiStyle['SkGray']['color'] = 'gray';%0a# $WikiStyle['SkSilver']['color'] = 'silver';%0a# $WikiStyle['SkMaroon']['color'] = 'maroon';%0a# $WikiStyle['SkPurple']['color'] = 'purple';%0a# $WikiStyle['SkNavy']['color'] = 'navy';%0a%0a##-- Skupiny a stránky vylúčené z vyhľadávania ----------------%0a%0a# $SearchPatterns['default'][] = '!\\.SkSearchWiki$!'; %0a# $SearchPatterns['default'][] = '!\\.Sk(All)?Recent(Changes|Uploads)$!';%0a# $SearchPatterns['default'][] = '!\\.SkHomePage$!';%0a# $SearchPatterns['default'][] = '!\\.SkSideBar$!';%0a# $SearchPatterns['allgroups'] = $SearchPatterns['default'];%0a# $SearchPatterns['default'][] = '!^PmWiki\\.!'; # vylúčenie skupiny PmWiki%0a# $SearchPatterns['default'][] = '!^AWikiGroup\\.!';%0a# etc.%0a%0a## Vylúčenie 