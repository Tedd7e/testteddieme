text=This page contains the conversion rules to convert PmWiki's prompts into another language. %0aSee [[(http://www.pmwiki.org/wiki/)Localization.Localization]] and [[PmWiki/Internationalizations]] for how to details.%0a%0aStrings have the original (English) text on the left-hand-side, and two quotation marks on the%0aright-hand-side which can be filled with the translation. %0a%0aSome strings already have an English explanation on the right-hand-side, which can be replaced with the appropriate translation, to explain the cryptic string names on the left-hand-side. %0a%0aLines commencing with the comment character "#" do not need to be translated.%0a%0aOther strings to be translated, including some used in previous versions of PmWiki, may be found at [[(http://www.pmwiki.org/wiki/)Localization.XLPageCookbookTemplate]].%0a%0a[@%0a  ### Locale identifier, e.g. 'de' or 'cs_CZ.ISO-8859-2'%0a  'Locale' => 'lv',%0a  ### Time format, e.g. '%25Y-%25m-%25d %25H:%25M EET'%0a  'TimeFmt' => '',%0a%0a  ### Standard layout strings%0a  'View' => 'Skatīt',%0a  'Edit' => 'Rediģēt',%0a  'History' => 'Vēsture',%0a  'Attach' => 'Pievienot failu',%0a  'Print' => 'Drukāt',%0a  'Backlinks' => '',%0a  'Login' => '',%0a  'Logout' => '',%0a  'Recent Changes' => 'Nesenās izmaiņas',%0a  'Search' => 'Meklēšana',%0a  'Page last modified on {$LastModified}' => 'Pēdējo reizi rediģēts {$LastModified}',%0a  'Go' => 'Aiziet',%0a  'All Recent Changes' => 'Visas nesenās izmaiņas',%0a  # access keys%0a  # 'ak_view' => '',%0a  # 'ak_edit' => 'e',%0a  # 'ak_history' => 'h',%0a  # 'ak_attach' => '',%0a  # 'ak_print' => '',%0a  # 'ak_backlinks' => '',%0a  # 'ak_logout' => '',%0a  # 'ak_recentchanges' => 'c',%0a%0a  ### PageTitle/LinkText technical pages (for full list see page Localization.Localization)%0a  'RecentChanges' => '',%0a  'AllRecentChanges' => '',%0a  'GroupHeader' => '',%0a  'GroupFooter' => '',%0a  'SideBar' => '',%0a  'GroupAttributes' => '',%0a%0a  ### Print layout strings%0a  'From $WikiTitle' => '$WikiTitle',%0a  'Retrieved from {$PageUrl}' => 'Saglabāts no {$PageUrl}',%0a%0a  ### Page locations%0a  '{$SiteGroup}/EditQuickReference' => '',%0a  '{$SiteGroup}/UploadQuickReference' => '',%0a  '{$SiteGroup}/Search' => '',%0a  '{$SiteGroup}.PageNotFound' => '',%0a%0a  ### Browse page strings%0a  'redirected from' => '',%0a  %0a  ### Edit page strings%0a  'Editing {*$FullName}' => '',%0a  'Save' => 'SaglabÄt',%0a  'Save and edit' => 'Saglabāt un rediģēt',%0a  'Preview' => 'Pirmskatīt',%0a  'Cancel' => 'Atcelt',%0a  'Reset' => '',%0a  'Author' => 'Autors',%0a  'An author name is required.' => '',%0a  'Summary' => '',%0a  'This is a minor edit' => '',%0a  'Preview {*$FullName}' => '',%0a  'End of preview -- remember to save' => '',%0a  'Page is unsaved' => '',%0a  'Top' => 'Augšup',%0a  # access keys%0a  # 'ak_save' => 's',%0a  # 'ak_saveedit' => 'u',%0a  # 'ak_preview' => 'p',%0a  # 'ak_savedraft' => 'd',%0a  # 'ak_textedit' => ',',%0a  # 'e_rows' => '23',%0a  # 'e_cols' => '60',%0a%0a  ### Page history strings%0a  '{$FullName} History' => '',%0a  'Show minor edits' => '',%0a  'Hide minor edits' => '',%0a  'Show changes to markup' => '',%0a  'Show changes to output' => '',%0a  'by' => '',%0a  'Restore' => '',%0a  'Added line $DiffLines:' => '',%0a  'Added lines $DiffLines:' => '',%0a  'Changed line $DiffLines from:' => '',%0a  'Changed lines $DiffLines from:' => '',%0a  'Deleted line $DiffLines:' => '',%0a  'Deleted lines $DiffLines:' => '',%0a  'to:' => '',%0a%0a  ### Page attribute strings%0a  'Attributes' => '',%0a  '{$FullName} Attributes' => '',%0a  'Set new read password:' => '',%0a  'Set new edit password:' => '',%0a  'Set new attribute password:' => '',%0a  'Set new publish password:' => '',%0a  'Set new upload password:' => '', %0a  '(set by $PWSource)' => '',%0a  '(using $PWCascade password)' => '',%0a  '(protected)' => '',%0a  'EnterAttributes' => "Enter new attributes for this page below.  Leaving a field blank will leave the attribute unchanged.  To clear an attribute, enter 'clear'.  To remove a password, enter 'nopass'.",%0a  'The page has an "attr" attribute and cannot be deleted.' => '',%0a%0a  ### Authorization strings%0a  'Name' => 'Vārds',%0a  'Password' => 'Parole',%0a  'Password required' => 'Parole ir obligāta',%0a  'Name/password not recognized' => 'Vārds vai parole nepareiza',%0a%0a  ### Search strings%0a  'Search Results' => 'Meklēšanas rezultāti',%0a  'SearchFor' => 'Results of searching for %3cem>$Needle%3c/em>',%0a  'SearchFound' => '$MatchCount pages found out of $MatchSearched pages searched.',%0a%0a  ### Upload strings%0a  'Attachments for' => '',%0a  'File to upload:' => 'Fails, kuru augšuplādēt',%0a  'Name attachment as:' => 'Nosaukt to par:',%0a  'Upload' => 'Uzlādēt',%0a  'Uploads' => 'Augšuplādes',%0a  'ULsuccess' => 'uzlādēts veiksmīgi',%0a  'ULbadname' => 'nekorekts faila nosaukums',%0a  'ULbadtype' => "'$upext' faila tips nav atļauts",%0a  'ULtoobig' => 'tā izmērs ir lielāks, nekā atļauj serveris',%0a  'ULtoobigext' => "faila izmērs ir lielāks par atļautajiem $upmax baitiem '$upext' tipa failiem",%0a  'ULpartial' => 'incomplete file received',%0a  'ULnofile' => 'no file uploaded',%0a  'ULexists' => 'file with that name already exists',%0a  'ULpquota' => 'group quota exceeded',%0a  'ULtquota' => 'upload quota exceeded',%0a%0a  ### GuiEdit button bar%0a  'Emphasized' => '',%0a  'Emphasized (italic)' => '',%0a  'Strong' => '',%0a  'Strong (bold)' => '',%0a  'Page link' => '',%0a  'Link to internal page' => '',%0a  'link text' => '',%0a  'Link to external page' => '',%0a  'file.ext' => '',%0a  'Attach file' => '',%0a  'Big text' => '',%0a  'Small text' => '',%0a  'Superscript' => '',%0a  'Subscript' => '',%0a  'Heading' => '',%0a  'Subheading' => '',%0a  'Center' => '',%0a  'Unordered list' => '',%0a  'Unordered (bullet) list' => '',%0a  'Ordered list' => '',%0a  'Ordered (numbered) list' => '',%0a  'Indented text' => '',%0a  'Hanging indent' => '',%0a  'Horizontal rule' => '',%0a  'Table' => '',%0a  # access keys%0a  # 'ak_em' => '',%0a  # 'ak_strong' => '',%0a%0a  ### Others%0a  '(approve sites)' => '',%0a  'This post has been blocked by the administrator' => '',%0a  'Address blocked from posting' => '',%0a  'Text blocked from posting' => '',%0a  'Password encryption' => '',%0a  'EditConflict' => "The page you are editing has been modified since you started editing it.  The modifications have been merged into the text below, you may want to verify the results of the merge before pressing save.  Conflicts the system couldn't resolve are bracketed by &lt;&lt;&lt;&lt;&lt;&lt;&lt; and &gt;&gt;&gt;&gt;&gt;&gt;&gt;.",%0a  'EditWarning' => "The page you are editing has been modified since you started editing it.  If you continue, your changes will overwrite any changes that others have made.",%0a  'View changes' => '',%0a  '?invalid page name' => '',%0a  'More information' => '',%0a  "PmWiki can't process your request" => '',%0a  'We are sorry for any inconvenience' => '',%0a  'Return to' => '',%0a%0a  ### Historical%0a  'Save as draft' => 'Saglabāt kā melnrakstu',  %0a  'not found' => 'nav atrasts',%0a%0a@]%0a%0a!!!Notes%0a
